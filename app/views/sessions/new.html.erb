<h1>Project Management System</h1>

<h2>Login</h2>
<%= form_tag sessions_new_path do %>
<div class="field">
  <%= label_tag "Username" %>
  <%= text_field_tag :username, params[:username] %>
</div>
<div class="field">
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</div>
<div class="actions">
  <%= submit_tag "Login" %>
</div>
<% end %>

