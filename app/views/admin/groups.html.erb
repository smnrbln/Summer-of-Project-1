<table>
  <tr>
    <th>Group ID</th>
    <th>Group Name</th>
    <th>Group Membrships</th>
    <th></th>
  </tr>
<% @groups.each do |g| %>
  <tr>
    <td><%= g.id %></td>
    <td><%= g.name %></td>
    <td><% g.user.each do |i| %>
                <%= i.id %>
                <%= i.name %>
                <%= i.surname %><br />
	<% end %></td>
<% end %>
</table>
